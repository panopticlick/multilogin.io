{
  "name": "@multilogin/client",
  "version": "1.0.0",
  "description": "Multilogin.io Desktop Client - Launch browser profiles with cloud sync",
  "main": "dist/index.js",
  "bin": {
    "multilogin": "./dist/cli.js"
  },
  "scripts": {
    "dev": "tsx watch src/cli.ts",
    "build": "tsup src/cli.ts src/index.ts --format esm,cjs --dts",
    "start": "node dist/cli.js",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "pkg:win": "pkg dist/cli.js --target node20-win-x64 --output dist/multilogin-win.exe",
    "pkg:mac": "pkg dist/cli.js --target node20-macos-x64 --output dist/multilogin-macos",
    "pkg:linux": "pkg dist/cli.js --target node20-linux-x64 --output dist/multilogin-linux",
    "pkg:all": "npm run pkg:win && npm run pkg:mac && npm run pkg:linux"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "conf": "^13.0.1",
    "inquirer": "^9.2.23",
    "node-fetch": "^3.3.2",
    "ora": "^8.0.1",
    "proxy-chain": "^2.4.0",
    "puppeteer": "^22.10.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "ws": "^8.17.0"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.12.0",
    "@types/ws": "^8.5.10",
    "pkg": "^5.8.1",
    "tsup": "^8.0.2",
    "tsx": "^4.10.5",
    "typescript": "^5.4.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "multilogin",
    "browser-profiles",
    "anti-detect",
    "puppeteer",
    "fingerprint"
  ],
  "author": "Multilogin.io",
  "license": "MIT"
}
